{
  "name": "database",
  "version": "0.0.1",
  "description": "Database module for ak-pra-backend",
  "repository": {
    "type": "git",
    "url": "https://git.app.kaercher.com/scm/ipp/ak-pra-backend.git"
  },
  "main": "dist/index.js",
  "types": "src/index.ts",
  "scripts": {
    "build": "tsc",
    "test": "npm run test:unit",
    "test:unit": "jest --verbose test/",
    "test:unit:watch": "npm run test:unit -- --watch --notify",
    "test:coverage": "npm run test:unit -- --coverage",
    "lint": "eslint src/",
    "lint:fix": "npm run lint -- --fix",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "migrate:generate": "npm run typeorm -- migration:generate",
    "migrate:up": "DB_TYPE=mysql DB_ENDPOINT_ADDRESS=localhost DB_PORT=3306 DB_NAME=kiracv3 DB_USERNAME=root LOG_LEVEL=trace node ./dist/runMigrations.js",
    "migrate:down": "DB_TYPE=mysql DB_ENDPOINT_ADDRESS=localhost DB_PORT=3306 DB_NAME=kiracv3 DB_USERNAME=root LOG_LEVEL=trace node ./dist/undoLastMigration.js"
  },
  "keywords": [],
  "author": {
    "name": "Nghia Nguyen",
    "email": "nghia.nguyen@zoi.de"
  },
  "license": "ISC",
  "dependencies": {
    "@automapper/classes": "^4.2.0",
    "@automapper/core": "^4.2.0",
    "mysql": "^2.18.1",
    "typeorm": "^0.2.33"
  },
  "devDependencies": {
    "@automapper/types": "^4.2.0",
    "@kaercher/ipp-cloud-common-lambda-libs": "1.0.2",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.9.0",
    "aws-sdk": "^2.933.0",
    "common": "file:../common",
    "inversify": "^5.1.1",
    "jest": "^27.0.5",
    "reflect-metadata": "^0.1.13",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.0.0",
    "typeorm": "^0.2.33",
    "typescript": "^4.3.4"
  }
}
